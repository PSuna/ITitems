<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE form-validation PUBLIC 
    "-//Apache Software Foundation//DTD Commons Validator Rules Configuration 1.1//EN" 
    "http://jakarta.apache.org/commons/dtds/validator_1_1.dtd">

<form-validation>

    <formset>
		<form name="carryRegist">
			<field property="id" depends="required">
        		<arg0 key="성명" resource="true"/>
			</field>	
			<field property="prjNm" depends="required">
        		<arg0 key="프로젝트" resource="true"/>
			</field>
			<field property="place" depends="required">
        		<arg0 key="사용장소" resource="true"/>
			</field>
			<field property="startDate" depends="required">
        		<arg0 key="반출기간(시작일)" resource="true"/>
			</field>
		</form>
    </formset>
    
	<formset>
		<form name="carryInRegist">
			<field property="id" depends="required">
        		<arg0 key="성명" resource="true"/>
			</field>	
			<field property="prjNm" depends="required">
        		<arg0 key="프로젝트" resource="true"/>
			</field>
		</form>
    </formset>
    
</form-validation>